<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon></icon>
	<name>Hepsini Pdf Yap</name>
	<submenu></submenu>
	<unique-id>1769756862757804-1</unique-id>
	<command>bash ~/dotfiles/scripts/mergeAllpdf.sh %F</command>
	<description>Seçili JPEG ve PDF dosyalarını tek PDF haline getirir</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
